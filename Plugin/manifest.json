{
  "manifest_version": 2,
  "name": "SafeSSL",
  "description": "Redirect http requests to https if possible",
  "version": "1.0",
  "background": {
    "scripts": ["checker.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions":[
      "<all_urls>",
      "webRequest",
      "webRequestBlocking",
      "tabs"
  ]
}
